{
    "name": "2ncorporate/cmcuapp",
    "type": "project",
    "description": "application de gestion hospitalière.",
    "keywords": [
        "Hospitalière",
        "Gestion"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-gd": "*",
        "askedio/laravel-soft-cascade": "^8.0",
        "barryvdh/laravel-dompdf": "^3.0",
        "bumbummen99/shoppingcart": "^5.0",
        "doctrine/dbal": "^3.0",
        "fideloper/proxy": "^4.4",
        "intervention/image": "^3.0",
        "laravel/framework": "^10.0",
        "laravel/tinker": "^2.8",
        "laravelcollective/html": "^6.3",
        "mercuryseries/flashy": "^1.0",
        "orangehill/iseed": "^3.0",
        "predis/predis": "^1.1",
        "spatie/laravel-image-optimizer": "^1.6"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9",
        "barryvdh/laravel-ide-helper": "^3.0,<3.2",
        "beyondcode/laravel-dump-server": "^2.0",
        "filp/whoops": "^2.15",
        "fakerphp/faker": "^1.23",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/factories",
            "database/seeds"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
